{
  "version": 2,
  "name": "davegames",
  "builds": [
    {
      "src": "davegames_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "davegames_project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "davegames_project.settings",
    "PYTHONPATH": ".",
    "DATABASE_URL": "@database_url",
    "SECRET_KEY": "@secret_key",
    "DEBUG": "False"
  },
  "functions": {
    "davegames_project/wsgi.py": {
      "includeFiles": "**"
    }
  },
  "regions": ["iad1"],
  "framework": null
}